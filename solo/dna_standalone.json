{
  "run_list": [
    "role[geoshape_core]"
  ],
  "ssl": false,
  "rogue": {
    "setup_db": true,
    "networking":{
      "application": {
        "fqdn": "{{fqdn}}"
       }
    },
    "geoserver_data":{
      "url":"{{gs-data-url}}",
      "branch":"{{gs-data-branch}}"
    },
    "django_maploom":{
      "auto_upgrade": true
    },
    "rogue_geonode":{
      "url":"{{gn-project-url}}",
      "branch":"{{gn-project-branch}}",
      "settings":{
        "CLASSIFICATION_BANNER_ENABLED": {{banner-on}},
        "CLASSIFICATION_TEXT_COLOR":"{{banner-color-text}}",
        "CLASSIFICATION_BACKGROUND_COLOR":"{{banner-color-background}}",
        "CLASSIFICATION_TEXT":"{{banner-text}}"
      }
    }
  },
  "tomcat":{
    "java_options": "-Djava.awt.headless=true -Xmx2g -XX:MaxPermSize=256m -XX:+UseConcMarkSweepGC"
  }
}
